{
  "name": "new-food-order-backend",
  "private": true,
  "scripts": {
    "postinstall": "cp -r node_modules/@volst/graphql-import node_modules/graphql-import/ && cp -r node_modules/@volst/graphql-import node_modules/graphql-yoga/node_modules/graphql-import/",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground --server-only --port=4005",
    "start-server": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "start": "npm-run-all --parallel start-server playground",
    "now-start": "node build/index.js",
    "precommit": "lint-staged",
    "prisma": "prisma",
    "deploy": "prisma deploy && prettier --write 'src/generated/**'",
    "lint": "tslint -p .",
    "test": "npm run -s lint && echo 'No real tests for now.'",
    "build": "rm -rf build && tsc",
    "now-build": "echo please zeit, do not build this"
  },
  "lint-staged": {
    "*.{js,json,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@volst/graphql-import": "^0.5.0",
    "bcryptjs": "2.4.3",
    "email-templates": "^3.3.3",
    "graphql-yoga": "1.9.2",
    "jsonwebtoken": "8.2.1",
    "nodemailer": "^4.4.2",
    "nodemailer-mailgun-transport": "^1.3.6",
    "prisma-binding": "1.5.17",
    "pug": "^2.0.0-rc.4",
    "speakingurl": "^14.0.1",
    "uuid": "^3.2.1",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/email-templates": "^3.1.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/nodemailer": "^4.3.1",
    "@types/nodemailer-mailgun-transport": "^1.3.3",
    "@types/speakingurl": "^13.0.2",
    "@types/uuid": "^3.4.3",
    "@types/validator": "^9.4.0",
    "@volst/tslint-config": "^0.2.1",
    "dotenv": "5.0.1",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "2.15.9",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "prisma": "1.6.3",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
